<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.doublea.binyutton">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity_requests_view"
            android:label="@string/title_activity_requests_view"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity_sign_up"
            android:label="@string/app_name" />
        <activity
            android:name=".activity_sign_in"
            android:label="@string/app_name" />
        <activity
            android:name=".activity_arrival"
            android:label="@string/title_activity_arrival"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>